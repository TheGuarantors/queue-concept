    "chai-as-promised": "^7.1.1",
    "chai-datetime": "^1.5.0",
    "mocha": "^5.1.1",
    "source-map-support": "^0.5.9",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.0.3"
  },
  "repository": {
    "type": "git",
