  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "config": {
    "tscheck": "node ../tscheck -r /src/ /dist/ ./src/"
  },
  "private": true,
  "engines": {
    "node": "^8.11.4"
  },
  "scripts": {
